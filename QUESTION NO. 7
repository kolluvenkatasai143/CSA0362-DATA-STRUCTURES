#include <stdio.h>

int main() {
    int arr[] = {4, 7, 2, 8, 5, 6, 9};
    int size = sizeof(arr) / sizeof(arr[0]);

    printf("Odd values: ");
    for (int i = 0; i < size; i++) {
        if (arr[i] % 2 != 0) {
            printf("%d ", arr[i]);
        }
    }

    printf("\nEven values: ");
    for (int i = 0; i < size; i++) {
        if (arr[i] % 2 == 0) {
            printf("%d ", arr[i]);
        }
    }

    return 0;
}
