#include <stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 2, 4}; // Example array
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Elements repeated twice: ");
    for (int i = 0; i < n; i++) {
        int count = 0;
        for (int j = 0; j < n; j++) {
            if (arr[i] == arr[j]) {
                count++;
            }
        }
        if (count == 2) {
            printf("%d ", arr[i]);
        }
    }

    return 0;
}
